<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.dei.unipd.it/db2/dcsso/"
     xml:base="http://www.dei.unipd.it/db2/dcsso/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:dcsso="http://www.dei.unipd.it/db2/dcsso#">
    <owl:Ontology rdf:about="http://www.dei.unipd.it/db2/dcsso">
        <owl:imports rdf:resource="http://www.w3.org/TR/skos-reference/skos-owl1-dl.rdf"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <rdfs:comment xml:lang="en">This ontology models the domain of footballers, and in particular of their detailed statistics through the years.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.dei.unipd.it/db2/dcsso#DataCrafters"/>
        <rdfs:label xml:lang="en">Datacrafters&apos; Soccer Statistics Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dei.unipd.it/db2/dcsso#forTeam -->

    <owl:ObjectProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#forTeam">
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
        <rdfs:range rdf:resource="http://www.dei.unipd.it/db2/dcsso#Team"/>
        <rdfs:comment xml:lang="en">Relates a seasonal membership of a player to the corresponding team.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#hasMembership -->

    <owl:ObjectProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#hasMembership">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
        <rdfs:range rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
    </owl:ObjectProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#hasTeam -->

    <owl:ObjectProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#hasTeam">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/member"/>
        <owl:inverseOf rdf:resource="http://www.dei.unipd.it/db2/dcsso#participatesIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#League"/>
        <rdfs:range rdf:resource="http://www.dei.unipd.it/db2/dcsso#Team"/>
    </owl:ObjectProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#participatesIn -->

    <owl:ObjectProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#participatesIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#Team"/>
        <rdfs:range rdf:resource="http://www.dei.unipd.it/db2/dcsso#League"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dei.unipd.it/db2/dcsso#assists -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#assists">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#games -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#games">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#goals -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#goals">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#keyPasses -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#keyPasses">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#minutes -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#minutes">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#npg -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#npg">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Non-penalty goals</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#npxG -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#npxG">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">The number of expected non-penalty goals of a player.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#npxG90 -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#npxG90">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Expected non-penalty goals per 90 minutes.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#redCards -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#redCards">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#role -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#role">
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>D</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>F</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>GK</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>M</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first>S</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#season -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#season">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">The season referred by a SeasonalMembership. Just the first year of the two (e.g. 2015/2016 is represented by 2015).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#shots -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#shots">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xA -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xA">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">The number of expected assists of a player.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xA90 -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xA90">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Expected assists per 90 minutes.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xG -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xG">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">The number of expected goals of a player, based on understat.com algorithm.</rdfs:comment>
        <rdfs:seeAlso>http://understat.com</rdfs:seeAlso>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xG90 -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xG90">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Expected goals per 90 minutes.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xGBuildup -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xGBuildup">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Total xG on all the possessions a player is involved in, not considering his shots and assists.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#xGChain -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#xGChain">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Total xG on all the possessions a player is involved in.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#yellowCards -->

    <owl:DatatypeProperty rdf:about="http://www.dei.unipd.it/db2/dcsso#yellowCards">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/nick"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dei.unipd.it/db2/dcsso#Defender -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#Defender">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#role"/>
                <owl:hasValue>D</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#Footballer -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#Footballer">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#role"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/name"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#Forward -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#Forward">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#role"/>
                <owl:hasValue>F</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#Goalkeeper -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#Goalkeeper">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#role"/>
                <owl:hasValue>GK</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#League -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#League">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/name"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#Midfielder -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#Midfielder">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#role"/>
                <owl:hasValue>M</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.dei.unipd.it/db2/dcsso#Footballer"/>
    </owl:Class>
    


    <!-- http://www.dei.unipd.it/db2/dcsso#SeasonalMembership -->

    <owl:Class rdf:about="http://www.dei.unipd.it/db2/dcsso#SeasonalMembership">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#assists"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#games"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#goals"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#keyPasses"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#minutes"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.dei.unipd.it/db2/dcsso#npg"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                    </owl:Restriction>
                    <owl:Restriction>
